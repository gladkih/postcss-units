{
  "rules": {
    "comma-dangle":          [2],
    "curly":                 [0],
    "indent":                [0],
    "key-spacing":           [0],
    "no-extra-parens":       [2],
    "no-loop-func":          [0],
    "no-multi-spaces":       [0],
    "no-underscore-dangle":  [0],
    "no-unused-expressions": [0],
    "no-unused-vars":        [2],
    "quotes":                [2, "single", "avoid-escape"],
    "strict":                [0],
  },
  "ecmaFeatures": {
  	"modules": true
  }
}
